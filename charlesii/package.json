{
  "name": "charlesii",
  "displayName": "CharlesII",
  "description": "AI-powered coding assistant that provide real-time coding support, insights, and interactions using a LLM ChatGPT-like model.",
  "version": "0.0.1",
  "license": "ISC",
  "publisher": "marlex",
  "repository": {
    "type": "git",
    "url": "https://github.com/marlecce/charlesII.git"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onCommand:extension.activate"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.optimizeWithCharlesII",
        "title": "Help me Charles II"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "extension.optimizeWithCharlesII",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "vsce": "^2.15.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@charlesII/engine": "file:../engine/dist",
    "openai": "^4.23.0"
  }
}
